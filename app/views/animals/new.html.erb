<%= form_for(@animal) do |f| %>

  <%= f.label :breed, 'Enter a breed'%>
  <%= f.text_field :breed %>


  <%= f.label :color, 'Enter color'%>
  <%= f.text_field :color %>

  <%= f.label :description, 'Enter description'%>
  <%= f.text_field :description %>

    <%= f.label :contact, 'Enter your phone number'%>
  <%= f.text_field :contact %>

  <%= f.label :location, 'Where di you find it ?'%>
  <%= f.text_field :location %>


  <%= f.submit %>
<% end %>
